{
  "name": "@thetaproom/strapi-sdk-javascript",
  "version": "0.5.1",
  "description": "The Taproom flavored version of the no longer maintained Strapi SDK for JavaScript",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-taproom/strapi--javascript-sdk.git"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "nyc --silent ava"
  },
  "engines": {
    "node": ">=12.*.*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.0",
    "@types/qs": "^6.5.1",
    "@types/sinon": "^5.0.5",
    "ava": "^1.0.0-rc.2",
    "browser-env": "^3.3.0",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "form-data": "^2.3.3",
    "npm-run-all": "^4.1.3",
    "npm-scripts-info": "^0.3.9",
    "nsp": "^3.2.1",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.2",
    "sinon": "^7.1.1",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/thetaproom/strapi-sdk-javascript/issues"
  },
  "homepage": "https://github.com/thetaproom/strapi-sdk-javascript#readme",
  "author": "The talented Strapi.io dev team"
}
